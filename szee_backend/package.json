{
  "name": "szee_backend",
  "version": "1.0.0",
  "description": "SZEE WebApp Backend",
  "main": "dist/index.js",
  "scripts": {
    "start": "node build/index.js",
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "build": "tsc",
    "dev": "concurrently \"nodemon -L index.ts\" \"nodemon -x tsoa spec\"",
    "swagger": "tsoa spec",
    "debug": "export DEBUG=* && npm run start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://ZarzadzanieEnergia@dev.azure.com/ZarzadzanieEnergia/System%20Zarz%C4%85dzania%20Energi%C4%85%20Elektryczn%C4%85/_git/SZEE%20-%20Backend"
  },
  "author": "Michał Pawłowski",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.25.3",
    "sequelize-typescript": "^2.1.5",
    "swagger-ui-express": "^4.5.0",
    "tsoa": "^4.1.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.3",
    "@types/sequelize": "^4.28.14",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^7.5.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node src/index.ts"
  }
}
